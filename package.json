{
  "name": "@svsticky/sticky-elements",
  "description": "Sticky web component library",
  "license": "GPL-3.0-only",
  "contributors": [
    {
      "name": "Jort Willemsen",
      "url": "https://www.npmjs.com/~jortw"
    },
    {
      "name": "Mees Maas",
      "url": "https://www.npmjs.com/~meesmaas"
    }
  ],
  "repository": {
    "url": "https://github.com/svsticky/sticky-elements"
  },
  "version": "0.0.1",
  "type": "module",
  "module": "./dist/lib/index.js",
  "exports": {
    ".": "./dist/lib/index.js",
    "./css": "./dist/lib/styles/styles.css"
  },
  "scripts": {
    "dev:watch": "concurrently \"tsc -w\" \"sass --watch ./lib/styles:./dist/lib/styles\" \"vite\"",
    "dev": "vite",
    "build": "tsc && sass ./lib/styles:./dist/lib/styles"
  },
  "dependencies": {
    "lit": "^3.1.4"
  },
  "devDependencies": {
    "@types/node": "^24.10.2",
    "concurrently": "^9.2.1",
    "path": "^0.12.7",
    "prettier": "^3.3.2",
    "sass": "^1.95.1",
    "sass-embedded": "^1.95.1",
    "tslib": "^2.8.1",
    "typescript": "^5.5.3",
    "url": "^0.11.4"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.5"
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  }
}
