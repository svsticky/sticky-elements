{
  "name": "@svsticky/sticky-elements",
  "description": "Sticky web component library",
  "license": "GPL-3.0-only",
  "contributors": [
    {
      "name": "Jort Willemsen",
      "url": "https://www.npmjs.com/~jortw"
    },
    {
      "name": "Mees Maas",
      "url": "https://www.npmjs.com/~meesmaas"
    }
  ],
  "version": "0.0.1",
  "type": "module",
  "main": "dist/src/index.js",
  "module": "dist/src/index.js",
  "exports": {
    ".": "./dist/src/index.js",
    "./css": "./dist/styles.css"
  },
  "scripts": {
    "dev": "tsc && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"web-dev-server\"",
    "build": "sass src/lib/styles/styles.scss dist/styles.css --style=compressed && tsc"
  },
  "dependencies": {
    "lit": "^3.1.4"
  },
  "devDependencies": {
    "@web/dev-server": "^0.4.6",
    "concurrently": "^9.2.1",
    "prettier": "^3.3.2",
    "sass": "^1.95.0",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3"
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  }
}
